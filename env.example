# BookNest Frontend Environment Variables
# Copy this file to .env and fill in the values

# Backend API URL
VITE_BASE_URL=https://library-management-application-beta.vercel.app

# Frontend URL (for CORS and redirects)
VITE_FRONTEND_URL=https://booknest-peach.vercel.app

# Optional: API Timeout (in milliseconds)
# VITE_API_TIMEOUT=10000

# Optional: Enable/disable debug mode
# VITE_DEBUG=false

# Optional: Environment (development, staging, production)
# VITE_ENV=production
